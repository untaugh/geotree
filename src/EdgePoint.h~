#pragma once
#include <Eigen/Core>
#include <set>
#include <iostream>
#include "Cube.h"

using namespace Eigen;

namespace Geotree
{
  enum MeshID
  {
    MESH0 = 0,
    MESH1 = 1,
  };

  class EdgePoint : public Point
  {
  public:
    int edge;
    double distance;
    bool operator < (const EdgePoint &edgepoint) const;
  };
  
  std::ostream& operator<< (std::ostream& stream, const Point& point);
}
